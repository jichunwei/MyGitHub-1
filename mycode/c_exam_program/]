#include<stdio.h>
int main()
{
    int i,m,n;
    for(i=2;i<m;i++)
    {
        for(i=2;i<n;i++)
        {
         if((m%i==0)&&(n%i==0))
         {printf("%d\n",i);}
        }
    }
    for(i=2;i<m;i++)
    {
        for(i=2;i<n;i++)
        {
            if(m*i==n*i)
            {printf("m*i=%d\n",m*i);}
        }
    }
}

